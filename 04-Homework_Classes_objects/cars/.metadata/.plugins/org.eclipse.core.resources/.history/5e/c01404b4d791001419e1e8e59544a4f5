package cars;

public class car {
	private int price;

	public car() {
		this.price = 0;
	}

	public car(int price) {
		this();
		setPrice(price);
	}

	public int getPrice() {

		return price;
	}

	public void setPrice(int price) {
		if (price >= 0) {
			this.price = price;
		}
	}

}