package zad2;

public abstract class stock {
	
	protected int price;
	protected boolean available;
	
	public int getPrice(){
		return price;
	}
	
	public void setPrice(){
		this.price =  price;
	}
	
	public boolean isAvailable()
	  {
	    return available;
	  }

	  public void setAvailable(boolean available)
	  {
	    this.available = available;
	  }

}
