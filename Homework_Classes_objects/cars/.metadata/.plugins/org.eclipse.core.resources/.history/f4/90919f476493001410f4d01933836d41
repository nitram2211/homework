package cars;

public class car {
	protected int price;
	protected String name;

	public car() {
		this.price = 0;
		this.name = null;
	}

	public car(int price) {

		setPrice(price);

	}

	public int getPrice() {

		return price;
	}

	public void setPrice(int price) {

		this.price = price;
	}

	public void carPrice() {
		System.out.printf("The car price is : %d. \n", this.price);

	}
}
